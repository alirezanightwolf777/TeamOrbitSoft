<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"

    tools:context=".Mazaheri.Mazaheri">

    <Button
        android:id="@+id/btmazstartforresult"
        android:layout_width="110dp"
        android:layout_height="50dp"
        android:layout_marginBottom="30dp"
        android:text="اجرای شرطی اینتنت"
        android:textSize="12sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/btmazstudent3"
        app:layout_constraintHorizontal_bias="0.555"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/btmazstudent"
        app:layout_constraintVertical_bias="0.073"
        tools:ignore="HardcodedText" />

    <Button
        android:id="@+id/btmaznamefamil"
        android:layout_width="110dp"
        android:layout_height="50dp"
        android:text="send name family"
        android:textSize="12sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.514"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/btmazstudent2"
        app:layout_constraintVertical_bias="0.086" />

    <Button
        android:id="@+id/btmazintent"
        android:layout_width="110dp"
        android:layout_height="50dp"
        android:text="@string/intent"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.913"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/btmazstudent2"
        app:layout_constraintVertical_bias="0.086" />

    <Button
        android:id="@+id/btmazstudent3"
        android:layout_width="110dp"
        android:layout_height="50dp"
        android:text="students 3"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/btmazstudent2"
        app:layout_constraintHorizontal_bias="0.939"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/btmazsix"
        app:layout_constraintVertical_bias="0.057"
        tools:ignore="HardcodedText" />

    <Button
        android:id="@+id/btmazstudent2"
        android:layout_width="110dp"
        android:layout_height="50dp"
        android:text="students 2"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.913"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/btmazsix"
        app:layout_constraintVertical_bias="0.057" />

    <Button
        android:id="@+id/btmazstudent"
        android:layout_width="110dp"
        android:layout_height="50dp"
        android:text="@string/students"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.083"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/btmazjadvis"
        app:layout_constraintVertical_bias="0.035" />

    <Button
        android:id="@+id/btmazaverage"
        android:layout_width="110dp"
        android:layout_height="50dp"
        android:text="میانگین شش عدد"
        android:textSize="12sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/btmazfard"
        app:layout_constraintHorizontal_bias="0.555"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView4maz"
        app:layout_constraintVertical_bias="0.031"
        tools:ignore="HardcodedText" />

    <Button
        android:id="@+id/btmazemail"
        android:layout_width="110dp"
        android:layout_height="50dp"
        android:text="ایمیل"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/btmazsix"
        app:layout_constraintHorizontal_bias="0.939"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/btmazfard"
        app:layout_constraintVertical_bias="0.035"
        tools:ignore="HardcodedText" />

    <Button
        android:id="@+id/btmazfard"
        android:layout_width="110dp"
        android:layout_height="50dp"
        android:text="اعداد فرد ۱ تا ۳۰"
        android:textSize="12sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/btmazjadvis"
        app:layout_constraintHorizontal_bias="0.939"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView4maz"
        app:layout_constraintVertical_bias="0.031"
        tools:ignore="HardcodedText" />

    <TextView
        android:id="@+id/textViewmaz"
        android:layout_width="100dp"
        android:layout_height="25dp"
        android:text="تمرینات"
        android:textAlignment="center"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.497"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.039"
        tools:ignore="HardcodedText" />

    <TextView
        android:id="@+id/textView4maz"
        android:layout_width="360dp"
        android:layout_height="360dp"


        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.49"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textViewmaz"
        app:layout_constraintVertical_bias="0.044" />

    <Button
        android:id="@+id/btmazret"
        android:layout_width="110dp"
        android:layout_height="50dp"
        android:layout_marginTop="0dp"
        android:layout_marginBottom="30dp"
        android:text="بازگشت"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/btmazstudent3"
        app:layout_constraintHorizontal_bias="0.555"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/btmazstartforresult"
        app:layout_constraintVertical_bias="0.137"
        tools:ignore="HardcodedText" />

    <Button
        android:id="@+id/btmazjadvis"
        android:layout_width="110dp"
        android:layout_height="50dp"
        android:layout_marginStart="8dp"
        android:text="@string/jadval"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.911"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView4maz"
        app:layout_constraintVertical_bias="0.035"
        tools:ignore="HardcodedText" />

    <Button
        android:id="@+id/btmazsix"
        android:layout_width="110dp"
        android:layout_height="50dp"
        android:text="@string/towg"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.913"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/btmazjadvis"
        app:layout_constraintVertical_bias="0.035" />
</androidx.constraintlayout.widget.ConstraintLayout>